{
  "name": "@outbound_iq/nextjs",
  "version": "0.1.1",
  "description": "OutboundIQ SDK for Next.js - Automatic tracking of all outbound API calls",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.mjs",
      "require": "./dist/register.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js"
    },
    "./edge": {
      "types": "./dist/edge.d.ts",
      "import": "./dist/edge.mjs",
      "require": "./dist/edge.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "outboundiq",
    "nextjs",
    "next.js",
    "api",
    "monitoring",
    "observability",
    "http",
    "fetch",
    "axios"
  ],
  "author": "OutboundIQ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/outboundiq-hq/nextjs-outboundiq.git"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "dependencies": {
    "@outbound_iq/core": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "next": "^14.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
